#include <stdio.h>
#include <string.h>

int main() {
    char s[1000];

    // Read the entire line (sentence)
    fgets(s, sizeof(s), stdin);

    // Tokenize the string by spaces
    char *token = strtok(s, " ");

    while (token != NULL) {
        printf("%s\n", token);
        token = strtok(NULL, " ");
    }

    return 0;
}
